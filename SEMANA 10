using System;
using System.Collections.Generic;
using System.Linq;

class ProgramaVacunacion
{
    static void Main()
    {
        // Crear los 500 ciudadanos ficticios
        var todos = new HashSet<string>();
        for (int i = 1; i <= 500; i++)
        {
            todos.Add($"Ciudadano {i}");
        }

        // Crear conjuntos de Pfizer y AstraZeneca
        var pfizer = new HashSet<string>();
        var astrazeneca = new HashSet<string>();
        Random rand = new Random();

        // Selección aleatoria de 75 ciudadanos para Pfizer
        while (pfizer.Count < 75)
        {
            int n = rand.Next(1, 501);
            pfizer.Add($"Ciudadano {n}");
        }

        // Selección aleatoria de 75 ciudadanos para AstraZeneca
        while (astrazeneca.Count < 75)
        {
            int n = rand.Next(1, 501);
            astrazeneca.Add($"Ciudadano {n}");
        }

        // 1. Ciudadanos no vacunados
        var vacunados = new HashSet<string>(pfizer);
        vacunados.UnionWith(astrazeneca);
        var noVacunados = new HashSet<string>(todos);
        noVacunados.ExceptWith(vacunados);

        // 2. Ciudadanos con ambas dosis
        var ambasDosis = new HashSet<string>(pfizer);
        ambasDosis.IntersectWith(astrazeneca);

        // 3. Solo Pfizer
        var soloP
